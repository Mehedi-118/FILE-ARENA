<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.file_arena">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SdCard"></activity>
        <activity android:name=".InternalStorage" />
        <activity android:name=".ImageViewScreen" />
        <activity android:name=".RecentWindow" />
        <activity android:name=".FavoriteWindow" />
        <activity android:name=".DownloadWindow" />
        <activity android:name=".AppsWindow" />
        <activity android:name=".ZipfolderWindow" />
        <activity android:name=".DocumentWindow" />
        <activity android:name=".AudioWindow" />
        <activity android:name=".VideoWindow" />
        <activity android:name=".GalleryWindow" />
        <activity android:name=".HomePage">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>